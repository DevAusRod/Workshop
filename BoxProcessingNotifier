javascript:(function(){let isBlue=!0,box=document.createElement("div");box.id="floatingBox",box.style.position="fixed",box.style.bottom="0",box.style.left="0",box.style.width="12px",box.style.height="12px",box.style.backgroundColor="blue",box.style.cursor="pointer",function toggleIndicator(){isBlue?box.style.backgroundColor="red":box.style.backgroundColor="blue",isBlue=!isBlue}(),document.body.appendChild(box)})();
